{
    "manifest_version": 3,
    "name": "Weather Check",
    "verison": "1.0.0",
    "description": "Simple weather applet to check the current weather!",
    "author": "Robby Rice",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage"
    ],
    "browser_action": {
        "default_popup": "weather-check/build/index.html"
    }
}